{
  "name": "When Not To Meet",
  "description": "Extends when2meet.com by determing when you're free from Google Calendar",
  "default_locale":"en",
  "version": "0.99",
  "background": {
    "page": "views/background.html"
  },
  "permissions": [
    "tabs", "activeTab", "http://*.google.com/", "https://*.google.com/"
   ],
  "browser_action": {
    "default_title": "Fill out when I can meet"
  },
  "icons": {
    "128": "images/icon-128.gif",
    "16":"images/icon-16.gif"
  },
  "content_scripts": [
      {
          "matches": ["http://*.when2meet.com/*", "https://*.when2meet.com/*"],
          "js": ["javascript/content.js"]
      }
  ],
  "manifest_version": 3
}
